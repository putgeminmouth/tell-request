{
    "manifest_version": 3,
    "name": "prmono",
    "description": "A Chrome Extension",
    "version": "0.0.0",
    "icons": {},
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*/*/pull/*"
            ],
            "js": [
                "pull.import.js"
            ],
            "css": [
                "src/pull.css"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "src/*"
            ],
            "matches": [
                "https://github.com/*"
            ]
        }
    ]
}